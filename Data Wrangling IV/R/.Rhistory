atualizar <- function(histórico) {
atual <- ((histórico + 17)/2)
return(atual)
}
atualizar (1)
atualizar (2)
atualizar (3)
atualizar (4)
atualizar_hoje <- c(1:15)
atualizar(atualizar_hoje)
atualizar(atualizar_hoje)
ajustar <- function(valor1, valor2) {
ajuste <- ((valor1 + 180)/(valor2 - 60))
return(ajuste)
}
ajustar(100, 80)
ajustar(200, 80)
ajustar(200, 100)
